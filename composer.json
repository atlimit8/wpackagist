{
    "name": "outlandish/wpackagist",
    "description": "Install and manage WordPress plugins with Composer",
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "7.4.*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-simplexml": "*",
        "babdev/pagerfanta-bundle": "^2.2",
        "composer/composer": "^1.10.10",
        "doctrine/dbal": "^2.10.2",
        "guzzlehttp/guzzle-services": "^1.1.3",
        "pagerfanta/pagerfanta": "^2.1.3",
        "rarst/wporg-client": "~0.5",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/flex": "^1.6",
        "symfony/form": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-csrf": "^5.0",
        "symfony/twig-pack": "^1.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "conflict": {
        "silex/silex": "*",
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "Outlandish\\Wpackagist\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
